{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": ["eslint:recommended", "plugin:import/recommended", "prettier"],
  "globals": {
    "cy": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "plugins": ["prettier", "svelte3"],
  "rules": {
    "no-console": "off",
    "prettier/prettier": "error"
  }
}
